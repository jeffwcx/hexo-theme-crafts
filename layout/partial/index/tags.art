<div class="widget widget--tags">
  <div class="widget_title">{{__('widget.tag')}}</div>
  <div class="widget_content">
    <div class="tags">
      <% site.tags.each(function(tag) { %>
      <div class="tag"><a href="{{url_for(tag.path)}}">{{tag.name}}</a></div>
      <% }) %>
    </div>
  </div>
</div>
