  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <%- algolia_search_config() %>
  {{block 'styles'}}{{/block}}
  <title>Hexo-Theme-crafts</title>
</head>
<body>
